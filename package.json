{
	"name": "cex-broker",
	"description": "Unified gRPC API to CEXs by Usher Labs",
	"repository": "git@gitlab.com:usherlabs/cex-broker.git",
	"homepage": "https://usher.so/",
	"author": "Oki Ayobami <oki@usher.so>",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@types/bun": "latest",
		"bun-plugin-dts": "^0.3.0",
		"bun-types": "latest",
		"husky": "^9.1.7"
	},
	"scripts": {
		"proto-gen": "./proto-gen.sh",
